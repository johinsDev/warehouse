{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "build/**/*.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
